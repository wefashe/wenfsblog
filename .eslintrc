{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "no-var": "error",
    "no-console": "off",
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "comma-spacing": [2, {
      "before": false,
      "after": true
    }]
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "html"
  ]

}
