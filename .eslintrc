{
  "extends": ["standard", "plugin:vue/essential", "plugin:prettier/recommended", "eslint:recommended"],
  "plugins": ["html"],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-new": "off",
    "no-tabs": "off",
    "no-console": "off",
    "no-unused-vars":"off",
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  }
}
